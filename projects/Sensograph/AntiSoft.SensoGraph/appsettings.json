{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "AppDatabase": "Data Source=sensograph.sqlite;Cache=Shared"
  },
  "Application": {
    "IntervalSecs": 60,
    "EnableMetricsRunner": "true",
    "DaysToKeepRecords": 90,
    "Metrics": [{
      "code": "random",
      "label": "test",
      "class": "RandomMetricProvider"
    }]
  }
}
